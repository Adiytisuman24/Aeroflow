{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "AeroFlow",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [{
        "name": "keyword.control.aeroflow",
        "match": "\\b(fn|let|return|if|else|agent|model|tensor|on|import|from|spawn)\\b"
      }, {
        "name": "support.function.aeroflow",
        "match": "\\b(print|len|type|range|spawn|sleep)\\b"
      }]
    },
    "strings": {
      "name": "string.quoted.double.aeroflow",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.aeroflow",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.aeroflow"
}
